<script>var contextPath = "${pageContext.request.contextPath}";</script>

<!-- 부트스트랩 -->
<link rel="stylesheet" href="${pageContext.request.contextPath}/css/login.css">
<!-- 합쳐지고 최소화된 최신 CSS -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css">
<!-- 부가적인 테마 -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap-theme.min.css">
<!-- 합쳐지고 최소화된 최신 자바스크립트 -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>

<!-- 부트스트랩 -->
 <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
  <link href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
  <!-- CSS Files -->
  <link href="${pageContext.request.contextPath}/assets/css/bootstrap.min.css" rel="stylesheet" />
  <link href="${pageContext.request.contextPath}/assets/css/paper-kit.css?v=2.2.0" rel="stylesheet" />
  <!-- CSS Just for demo purpose, don't include it in your project -->
  <link href="${pageContext.request.contextPath}/assets/demo/demo.css" rel="stylesheet" />
</head>
<body onkeydown ="enter()">         

<div class="login-wrap">
   <div class="login-html">
      <input id="tab-1" type="radio" name="tab" class="sign-in" checked><label for="tab-1" class="tab">Sign In</label>
      <input id="tab-2" type="radio" name="tab" class="for-pwd"><label for="tab-2" class="tab">비밀번호 찾기</label>
       <input id="tab-3" type="radio" name="tab" class="for-email"><label for="tab-3" class="tab">이메일 찾기</label>
       
            <div class="login-form">
               <!-- 로그인 -->
                <div class="sign-in-htm" style="margin-top: 30px;">
                    <div class="group" >
                        <input id="email" name="email" type="email" class="input"  placeholder="이메일">
                    </div>
                    <div class="group">
                        <input id="pass" type="password" name="password" class="input" data-type="password"  placeholder="비밀번호">
                    </div>
                    <div class="group">
                        <input type="button" id="loginBtn" class="button" value="Login" onclick="login();">
                    </div>
                    <label><input type="checkbox" id="emailSave">아이디 저장</label>
               <br><br>
               
               <div class="hr"></div>
               <p>아직 회원이 아니세요? <a href="${pageContext.request.contextPath}/join/step1">회원가입하기</a>
                    <div class="hr"></div>

                </div>
                
                <!-- 비밀번호 찾기 -->
                <div class="for-pwd-htm" style="margin-top: 30px;"></div>
                
                <!-- 이메일 & 비밀번호 찾기 -->
                <div class="for-email-htm" style="margin-top: 30px;">

                </div>
            </div>

      </div>
      
   </div>


         <div id="homelogo">
            <a href="<c:url value="/index" />"><img src="${pageContext.request.contextPath}/imagefile/12.gif"></a>
         </div>   




<script src="${pageContext.request.contextPath}/js/login.js" charset="UTF-8"></script>
   

</body>

</html>